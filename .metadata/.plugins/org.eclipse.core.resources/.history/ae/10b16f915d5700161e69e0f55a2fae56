package vector_meses;

public class proceso {

	public proceso(String a[])
	{	
	}
	
	public void mostrar(String a[],int meses)
	{
	  System.out.println("************************************************************************");
	  System.out.print("  *TU MES SEGUN EL NUMERO INGRESADO ES : "+"\n"+a[meses-1]);	
	  System.out.println("************************************************************************");
	}
}
